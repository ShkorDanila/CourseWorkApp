<Page x:Class="CourseWorkApp.LevelSelectionPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CourseWorkApp"
      mc:Ignorable="d" 
      d:DesignHeight="720" d:DesignWidth="1600"
      Title="LevelSelectionPage">

    <Grid Background="White">
        <Button Content="Button" Margin="0,50,60,0" VerticalAlignment="Top" HorizontalAlignment="Right" RenderTransformOrigin="0.489,0.676" Height="28" Width="124" Click="startLevelButton_Click" />
        <Button Content="Back to Main Menu" Margin="0,100,60,0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="124" Height="28" Click="backToMMButton_Click" />
        <ListView x:Name="listOfLevels" Margin="26,18,244,33" >
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Page>
